{
  "name": "graph-web",
  "version": "1.0.0",
  "description": "An app to draw graphs.",
  "main": "main.js",
  "scripts": {
    "webpack": "cross-env NODE_DEV=true webpack",
    "webpack-watch": "cross-env NODE_DEV=true webpack --watch",
    "start": "cross-env NODE_DEV=true electron .",
    "prebuild": "cross-env NODE_DEV=false webpack",
    "build-win32": "electron-packager . Graph --platform=win32 --arch=ia32 --electron-version=3.1.1 --app-version=1.0.0 --out=build --ignore=build --ignore=node_modules --ignore=scripts --ignore=styles --overwrite",
    "build-linux": "electron-packager . Graph --platform=linux --arch=ia32 --electron-version=3.1.1 --app-version=1.0.0 --out=build --ignore=build --ignore=node_modules --ignore=scripts --ignore=styles --overwrite"
  },
  "author": "Jed Dark",
  "repository": {
    "type": "git",
    "url": "https://github.com/darkjedi9922/graph-web"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "3.1.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "13.0.1",
    "electron-prebuilt": "1.4.13",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "glob": "^7.1.4",
    "node-sass": "^4.12.0",
    "node-sass-once-importer": "^5.3.2",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-contextmenu": "^2.11.0",
    "react-dom": "^16.8.6",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "url-loader": "^2.0.0",
    "watchify": "^3.11.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2"
  }
}
